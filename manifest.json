{
  "name": "Taghash",
  "version": "1.0",
  "description": "Add deals to taghash directly from various websites now!",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}