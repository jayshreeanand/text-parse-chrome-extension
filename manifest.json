{
  "name": "Taghash",
  "version": "1.0",
  "description": "Add deals to taghash directly from various websites now!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/logo.png"

  },
  "manifest_version": 2
}